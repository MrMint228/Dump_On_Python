word = str(input("–í–≤–µ–¥–∏—Ç–µ —Å–ª–æ–≤–æ:\n"))
shift = int(input("–í–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ —Å–¥–≤–∏–≥–∞:\n"))
cesar_word = ""
print("–®–∏—Ñ—Ä—É–µ–º –∫–∞–∫ –¶–µ–∑–∞—Ä—å...")
print("=" * 20)
for i in word:
    cesar_word += chr(ord(i)+shift)
print(cesar_word)
print("=" * 20)
